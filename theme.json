{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#9ca57a"
				},
				{
					"name": "Background",
					"slug": "background",
					"color": "#f2dfc2"
				},
				{
					"name": "Foreground",
					"slug": "foreground",
					"color": "#050505"
				},
				{
					"name": "Accent",
					"slug": "accent",
					"color": "#ffffff"
				}
			],
			"link": true
		},
		"layout": {
			"contentSize": "640px",
			"wideSize": "1280px"
		},
		"typography": {
			"customFontSize": true,
			"lineHeight": true,
			"fontFamilies": [
				{
					"fontFamily": "\"Oranienbaum\", \"Arial Narrow\", Arial, sans-serif",
					"slug": "oranienbaum",
					"name": "Oranienbaum",
					"google": "family=Oranienbaum&display=swap"
				},
				{
					"fontFamily": "\"EB Garamond\", Georgia, Times, \"Times New Roman\", serif",
					"slug": "eb-garamond",
					"name": "EB Garamond",
					"google": "family=EB+Garamond:ital@0;1&display=swap"
				}
			],
			"fontSizes": [
				{
					"size": "0.5rem",
					"slug": "small"
				},
				{
					"size": "1rem",
					"slug": "medium"
				},
				{
					"size": "2rem",
					"slug": "large"
				},
				{
					"size": "3rem",
					"slug": "x-large"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oranienbaum)",
					"textTransform": "uppercase"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oranienbaum)",
					"textTransform": "uppercase"
				}
			},
			"core/button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--foreground)",
					"text": "var(--wp--preset--color--background)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oranienbaum)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"textTransform": "uppercase"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oranienbaum)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "400",
					"textTransform": "uppercase"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oranienbaum)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "400",
					"textTransform": "uppercase"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oranienbaum)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "400",
					"textTransform": "uppercase"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--eb-garamond)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "400"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--eb-garamond)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "400"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--eb-garamond)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "400"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--eb-garamond)",
			"fontSize": "var(--wp--preset--font-size--medium)"
		}
	}
}
